{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "forbidden_arcanus:block/thin_cherrywood_log_horizontal",
        "y": 90
      }
    },
    {
      "when": {
        "axis": "y"
      },
